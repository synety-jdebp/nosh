[Unit]
Description=Service manager

[Service]
ExecStart=/bin/exec move-to-control-group systemd.slice service-manager
TimeoutSec=10
# systemd's default file handle limit of 1024 is far too low for normal usage patterns.
LimitNOFILE=16384
